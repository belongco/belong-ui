{
	"scripts": {
		"lint": "eslint --ext .jsx --ext js ./packages/",
		"styleguide": "styleguidist server --config ./styleguide/config.js",
		"styleguide:build": "styleguidist build --config ./styleguide/config.js mv ./styleguide/styleguide/* ./",
		"publish:npm": "lerna exec --parallel yarn run build && lerna publish --skip-git --registry=https://registry.npmjs.org/"
	},
